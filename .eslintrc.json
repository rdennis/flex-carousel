{
    "root": true,
    "env": {
        "es6": true,
        "browser": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "no-console": [
            "warn", // "error",
            {
                "allow": [
                    "warn",
                    "error"
                ]
            }
        ],
        "valid-jsdoc": [
            "warn",
            {
                "requireReturn": false
            }
        ]
    }
}